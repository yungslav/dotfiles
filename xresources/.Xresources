! Black color that will not be affected by bold highlighting.
*.color66: #272e38
*color66:  #272e38


URxvt.font:xft:SourceCodeProSemiBold:pixelsize=14
URxvt*depth: 32
URxvt*background: rgba:0000/0000/0200/c800
URxvt.transparent: true
URxvt.shading: 40s
URxvt.perl-ext: default,matcher
URxvt.url-launcher: firefox
URxvt.matcher.button: 3
URxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
URxvt.matcher.pattern.2: (?<=\\s)`(?=\\w)([^']+)(?<=\\w)'
URxvt.matcher.launcher.2: /home/matus/.local/bin/emacs-show-docs $1
URxvt.keysym.M-Delete: perl:matcher:list

!Home and End keys to jump to the beginning and end of lines
URxvt*keysym.Home:       \033[1~
URxvt*keysym.End:        \033[4~

!! Perl extensions
! url-select (part of urxvt-perls package)
URxvt.perl-ext-common:      default,clipboard,matcher,keyboard-select
URxvt.keysym.M-u: perl:     url-select:select_next
URxvt.url-select.autocopy:  true
URxvt.url-select.button:    2
URxvt.url-select.underline: true
URxvt.url-launcher:         /usr/bin/firefox
URxvt.matcher.button:       1

!! CopyPaste Keys
URxvt.keysym.Shift-Control-V:   perl:clipboard:paste
URxvt.keysym.Shift-Control-C:   perl:clipboard:copy

!! Disable the stupid ctrl+shift 'feature'
URxvt.iso14755:            false
URxvt.iso14755_52:         false

!! Hide Scrollbar
URxvt.scrollBar: false
